# utilities
import re

def clean_tweets(tweet):
    # Replace multiple spaces with single spaces.
    tweet = re.sub(' +', ' ', tweet)
    # Convert two or more dot's into one.
    tweet = re.sub(r'\.+', ".", tweet)
    # replace "&amp" into &
    tweet = tweet.replace("&amp;", "&")

    return tweet

